{
	"name": "states-nepal",
	"version": "0.2.0",
	"description": "Get structured dataset about administrative division in Nepal.",
	"keywords": [
		"states-nepal",
		"nepal",
		"nepali",
		"district-nepal",
		"municipality-nepal",
		"province-nepal"
	],
	"main": "./lib/cjs/src/index.js",
	"module": "./lib/esm/src/index.js",
	"types": "./lib/esm/src/index.d.ts",
	"files": [
		"lib/"
	],
	"repository": "https://github.com/adarshaacharya/states-nepal.git",
	"author": "Aadarsha <connectwithadarsha@gmail.com>",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"license": "MIT",
	"scripts": {
		"test": "jest",
		"build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
		"build:watch": "tsc -w",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"prepublishOnly": "npm run build"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn format && yarn test"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.22",
		"husky": "^4.3.7",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"ts-jest": "^26.4.4",
		"typescript": "^4.1.3"
	}
}
